@techreport{orig_smear,
	Abstract = {As each Kepler frame is read out, light from each star in a CCD column accumulates in successive pixels as they wait for the next row to be read out. This accumulation is the same in the masked rows at the start of the readout and virtual rows at the end of the readout as it is in the science data. A range of these "smear" rows are added together for each long cadence and sent to the ground for calibration purposes. We will introduce and describe this smear collateral data, discuss and demonstrate its potential use for scientific studies exclusive of Kepler calibration.},
	Author = {J.J. Kolodziejczak and D.A. Caldwell},
